set -e
set -v
for i in $(find -name \*.cpp | grep -v Robot); do
	echo "filename:" $i
	arm-frc-linux-gnueabi-g++ -std=c++1y -I$HOME/wpilib/cpp/current/include "-I/home/software2/workspace/Arcade Drive/src" -O0 -g3 -Wall -c -fmessage-length=0 -o $i.o $i
done
arm-frc-linux-gnueabi-g++ -L$HOME/wpilib/cpp/current/lib -Wl,-rpath,/opt/GenICam_v2_3/bin/Linux_armv7-a -o FRCUserProgram $(find -name \*.o) -lwpi
